                                       IsVisible="{Binding Index[3][3].Value, Converter={x:Static ObjectConverters.IsNull}}"
                                       Text="{Binding Path=Index[3][3].possibleValues[3] }" />
                        </Grid>
                    </Border>
                </Grid>

            </Border>
        </Grid>
        <Button Command="{Binding SolveNextStep}">Step</Button>
    </StackPanel>

</Window>